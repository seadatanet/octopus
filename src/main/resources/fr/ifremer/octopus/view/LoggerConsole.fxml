<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox prefHeight="200.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.ifremer.octopus.view.LoggerConsoleController">
   <children>
      <ListView fx:id="listViewLog" />
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
            <Button mnemonicParsing="false" onAction="#handleRemoveSelected" text="Remove selected" />
            <Button fx:id="buttonClearLog" mnemonicParsing="false" onAction="#handleClearLog" text="Clear" />
            <ToggleButton fx:id="toggleButtonAutoScroll" mnemonicParsing="false" text="Auto-Scroll" />
            <ChoiceBox fx:id="choiceBoxLogLevel" prefWidth="150.0" />
        </items>
      </ToolBar>
   </children>
</VBox>