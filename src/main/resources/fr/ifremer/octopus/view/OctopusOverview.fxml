<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.ifremer.octopus.view.OctopusOverviewController">
	<children>
		<SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="550.0" prefWidth="693.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<items>
				<AnchorPane minHeight="-Infinity" minWidth="0.0">
					<children>
                  <VBox AnchorPane.bottomAnchor="9.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                     <children>
                        <GridPane alignment="CENTER" hgap="5.0" prefHeight="188.0" vgap="5.0">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="297.0" />
                            <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="683.0" />
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="200.0" prefWidth="200.0" />
                          </columnConstraints>
                          <rowConstraints>
                              <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="ALWAYS" />
                              <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="ALWAYS" />
                            <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="ALWAYS" />
                            <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="ALWAYS" />
                            <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="ALWAYS" />
                          </rowConstraints>
                           <children>
                              <Label text="%output.type" GridPane.rowIndex="2" />
                              <Label text="%output.label" GridPane.rowIndex="3" />
                              <TextField fx:id="outputPathTextField" prefHeight="25.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                              <Button fx:id="chooseOut" mnemonicParsing="false" onAction="#openChooseOut" text="%output.chooseOut" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                              <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                                 <children>
                                    <RadioButton fx:id="radioMono" mnemonicParsing="false" onAction="#checkedMono" text="%output.type.mono">
                                       <toggleGroup>
                                          <ToggleGroup fx:id="outputType" />
                                       </toggleGroup>
                                    </RadioButton>
                                    <RadioButton fx:id="radioMulti" mnemonicParsing="false" onAction="#checkedMulti" selected="true" text="%output.type.multi" toggleGroup="$outputType" />
                                 </children>
                              </HBox>
            						<Label text="%input.label">
                                 <GridPane.margin>
                                    <Insets right="2.0" />
                                 </GridPane.margin>
                                 <padding>
                                    <Insets right="3.0" />
                                 </padding>
                              </Label>
            						<TextField fx:id="inputPathTextField" onKeyPressed="#inputChanged" prefHeight="32.0" prefWidth="691.0" GridPane.columnIndex="1" />
                              <Button fx:id="checkButton" mnemonicParsing="false" onAction="#validate" text="%input.validate" GridPane.columnIndex="2" />
                              <Label prefHeight="30.0" prefWidth="311.0" text="%output.outputLocalCdiId" GridPane.rowIndex="4" />
                              <TextField fx:id="outCDI" prefHeight="25.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                              <Button fx:id="chooseOutCdi" mnemonicParsing="false" onAction="#openChooseOutCdi" text="%output.chooseOutCDI" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </GridPane>
                        <HBox prefHeight="198.0" prefWidth="1188.0">
                           <children>
                              <VBox HBox.hgrow="ALWAYS">
                                 <children>
                            <CheckBox fx:id="showCdi" mnemonicParsing="false" onAction="#showCdiList" prefHeight="36.0" prefWidth="236.0" text="%input.checkCdi">
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                       </padding>
                                    </CheckBox>
                                    <VBox fx:id="cdiContainer" prefHeight="180.0" prefWidth="676.0" visible="false" />
                                 </children>
                              </VBox>
                              <VBox spacing="5.0">
                                 <children>
                                    <VBox alignment="CENTER" prefWidth="150.0" spacing="5.0">
                                       <children>
                                          <Label text="%output.export">
                                             <font>
                                                <Font size="30.0" />
                                             </font>
                                          </Label>
                                          <Button fx:id="buttonExportMedatlas" mnemonicParsing="false" onAction="#exportToMed" prefHeight="31.0" prefWidth="99.0" text="%output.medatlas" />
                                          <Button fx:id="buttonExportOdv" mnemonicParsing="false" onAction="#exportToODV" prefHeight="31.0" prefWidth="99.0" text="%output.odv" />
                                          <Button fx:id="buttonExportCfpoint" mnemonicParsing="false" onAction="#exportToCfPoint" prefHeight="31.0" prefWidth="99.0" text="%output.cfpoint" />
                                       </children>
                                    </VBox>
                                 </children>
                              </VBox>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
					</children>
				</AnchorPane>
				<fx:include source="LoggerConsole.fxml" />
			</items>
		</SplitPane>
	</children>
</AnchorPane>
