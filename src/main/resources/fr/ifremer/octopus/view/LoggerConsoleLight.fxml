<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.ifremer.octopus.view.LoggerConsoleControllerLight">
   <children>
      <ListView fx:id="listViewLog" VBox.vgrow="ALWAYS" />
      
       <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
            <Button mnemonicParsing="false" onAction="#handleRemoveSelected" text="%log.remove" />
            <Button fx:id="buttonClearLog" mnemonicParsing="false" onAction="#handleClearLog" text="%log.clear" />
            <Button fx:id="buttonScrollLog" mnemonicParsing="false" onAction="#handleScrollToEndLog" text="%log.scroll" />
            <Label text="%log.level" />
            <ChoiceBox fx:id="choiceBoxLogLevel" prefWidth="150.0" />
        </items>
      </ToolBar>
      
      
   </children>
</VBox>
