<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="550.0" prefWidth="693.0" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.ifremer.octopus.view.OctopusOverviewController">
	<children>
		<SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="550.0" prefWidth="693.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<items>
				<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
					<children>
						<Label layoutX="5.0" layoutY="14.0" text="%input.label" AnchorPane.leftAnchor="5.0" />
						<TextField fx:id="inputPathTextField" layoutX="5.0" layoutY="39.0" prefHeight="25.0" prefWidth="520.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" />
                  <Label layoutX="14.0" layoutY="77.0" text="%output.label" AnchorPane.leftAnchor="5.0" />
                  <TextField fx:id="outputPathTextField" layoutX="5.0" layoutY="102.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" />
                  <Label layoutX="5.0" layoutY="143.0" text="%output.type" />
                  <RadioButton fx:id="radioMono" layoutX="202.0" layoutY="142.0" mnemonicParsing="false" text="%output.type.mono">
                     <toggleGroup>
                        <ToggleGroup fx:id="outputType" />
                     </toggleGroup>
                  </RadioButton>
                  <RadioButton fx:id="radioMulti" layoutX="346.0" layoutY="142.0" mnemonicParsing="false" selected="true" text="%output.type.multi" toggleGroup="$outputType" />
                  <Label layoutX="19.0" layoutY="195.0" text="%output.export" AnchorPane.leftAnchor="19.0">
                     <font>
                        <Font size="19.0" />
                     </font>
                  </Label>
                  <Button layoutX="187.0" layoutY="184.0" mnemonicParsing="false" onAction="#exportToMed" prefHeight="46.0" prefWidth="150.0" text="%output.medatlas" />
                  <Button layoutX="343.0" layoutY="184.0" mnemonicParsing="false" onAction="#exportToODV" prefHeight="46.0" prefWidth="150.0" text="%output.odv" />
                  <Button layoutX="499.0" layoutY="184.0" mnemonicParsing="false" onAction="#exportToCfPoint" prefHeight="46.0" prefWidth="150.0" text="%output.cfpoint" />
					</children>
				</AnchorPane>
				<fx:include source="LoggerConsole.fxml" />
			</items>
		</SplitPane>
	</children>
</AnchorPane>
